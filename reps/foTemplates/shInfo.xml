                                    <fo:block xdofo:xliff-note="COUNTMETERS,  E" orphans="2" widows="2" linefeed-treatment="preserve" start-indent="0.0pt" text-align="center" padding-bottom="0.0pt" end-indent="0.0pt" padding-top="0.0pt" height="0pt">
                                       <fo:inline font-style="italic" font-family="Courier New" height="12.0pt" font-weight="bold" font-family-generic="modern" white-space-collapse="false" style-name="Normal" font-size="12.0pt">
                                          <xsl:value-of select="(.//COUNTMETERS)[1]" xdofo:field-name="COUNTMETERS"/>
                                       </fo:inline>
