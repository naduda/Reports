               <fo:block xdofo:line-spacing="multiple:13.8pt" padding-bottom="10.0pt" text-align="center" linefeed-treatment="preserve" orphans="2" padding-top="0.0pt" widows="2" start-indent="5.4pt" end-indent="5.4pt" height="0.0pt" font-family="Courier New" font-size="14.0pt"/>
               <fo:block xdofo:line-spacing="multiple:13.8pt" padding-bottom="10.0pt" text-align="center" linefeed-treatment="preserve" orphans="2" padding-top="0.0pt" widows="2" start-indent="5.4pt" end-indent="5.4pt" height="0.0pt" font-family="Courier New" font-size="14.0pt">
                  <fo:inline id="{concat('page-total-', $_SECTION_NAME, $_XDOFOPOS)}"/>
